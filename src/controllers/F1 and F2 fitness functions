# In demo_ctrl.cpp the fitness function is for F3. Copy and paste the following functions in demo_ctrl.cpp from lines 72-94

#F1 fitness function:

if (in.size()) {

	distance =  1;
  m_fPerformance += calcPerformance(getLUTIndex(distance));
   
}

else {
  distance = 0;
}

#F2 fitness function:

if (in.size()) {

	distance =  in.size();
	m_fPerformance += calcPerformance(getLUTIndex(distance)); 
  
}

else {
  distance = 0;
}
